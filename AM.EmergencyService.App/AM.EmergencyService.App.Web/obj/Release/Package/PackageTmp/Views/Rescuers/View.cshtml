@model IEnumerable<AM.EmergencyService.App.Web.Models.RescuerViewModel>

<table class="table">
    <tr>
        Cостав бригады:
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Удалить", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
