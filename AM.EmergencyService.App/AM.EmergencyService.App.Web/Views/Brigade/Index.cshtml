@model IEnumerable<AM.EmergencyService.App.Common.Models.BrigadeModel>
@{
    ViewBag.Title = "Бригады";
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BrigadeNumber)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BrigadeNumber)
            </td>
            <td>
                @Html.ActionLink("Редактировать", "Edit", new { brigadeNumber = item.BrigadeNumber }) |
                @Html.ActionLink("Подробнее", "Details", new { brigadeNumber = item.BrigadeNumber }) |
               @* @Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Сформировать бригаду", "Create")